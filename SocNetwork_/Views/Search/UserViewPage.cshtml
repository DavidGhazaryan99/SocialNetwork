                </div>
                <div class="media-right text-black">
                    @if (Model.SignInUser.Id == Model.UserView.Id)

                    }
                    @if (Model.ThisIsFriend == false && Model.SignInUser.Id != Model.UserView.Id && Model.FriendRequest == null)
                    {
                        <input type="button" value="Add Friend" onclick="location.href='@Url.Action("AddFriend", "Search", new {id=Model.UserView.Id })'" />
                    }
                    @if (Model.ThisIsFriend == true && Model.SignInUser.Id != Model.UserView.Id && Model.FriendRequest == null)
                    {
                        <input type="button" value="delete" onclick="location.href='@Url.Action("DeleteFriend", "Friend", new {id=Model.UserView.Id })'" />
                    }
                    @if (Model.FriendRequest == "false")
                    {
                        <h4 style="color:white">You already sent friend request</h4>
                    }
                    @if (Model.FriendRequest == "true")
                    {
                        <input type="button" value="Accept" onclick="location.href='@Url.Action("Accept", "Search", new { id = Model.UserView.Id })'" />
                        <input type="button" value="Decline" onclick="location.href='@Url.Action("Decline", "Search", new { id = Model.UserView.Id })'" />
                    }
                    @if (Model.FriendRequest == null)
                    {

                    }

                </div>
            </div>
        </div>


